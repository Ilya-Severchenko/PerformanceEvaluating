@model IEnumerable<PerformanceEvaluating.Data.Models.RequestResult>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="table-responsive">
    <table class="table table-striped table-bordered">
        <tr>
            <th>Url</th>
            <th>Response time</th>
            <th>StatusCode</th>
        </tr>
        @foreach (var requestResult in Model)
        {
            <tr>
                <td>@requestResult.Url</td>
                <td>@requestResult.Attempt</td>
                <td>@requestResult.StatusCode</td>
            </tr>
        }
    </table>
</div>